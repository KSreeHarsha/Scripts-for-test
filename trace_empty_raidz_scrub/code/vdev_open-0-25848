  vdev_open_child() {
    vdev_open() {
      vdev_get_min_asize();
      vdev_raidz_open() {
        vdev_open_children() {
          vdev_uses_zvols() {
            zvol_is_zvol();
            vdev_uses_zvols() {
              zvol_is_zvol();
            }
            vdev_uses_zvols() {
              zvol_is_zvol();
            }
          }
        }
      }
      vdev_set_state();
      vdev_set_min_asize() {
        vdev_get_min_asize();
        vdev_set_min_asize() {
          vdev_get_min_asize();
        }
        vdev_set_min_asize() {
          vdev_get_min_asize();
        }
      }
    }
  }
